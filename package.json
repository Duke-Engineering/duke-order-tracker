{
  "name": "duke-order-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "codegen:schema": "npx apollo service:download --endpoint=https://graph.staging.dukesell.dev/ src/graphql/schema.json",
    "codegen:generate": "npx apollo client:codegen --localSchemaFile=src/graphql/schema.json --globalTypesFile=src/graphql/__generated__/globalTypes.ts --includes=src/graphql/**/*.ts --target=typescript",
    "cd": "yarn graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@heroicons/react": "^2.0.18",
    "@reduxjs/toolkit": "^1.9.7",
    "graphql": "^16.8.1",
    "js-cookie": "^3.0.5",
    "localforage": "^1.10.0",
    "moment": "^2.29.4",
    "ramda": "^0.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.19.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.9.2",
    "@types/ramda": "^0.29.9",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
